<?xml version="1.0" encoding="utf-8"?>
<Settings>
  <!--Name of public type inheriting $RootNamespace$.T4WebFormsInjection.IScopedContainer.-->
  <ContainerType>T4WFI.App.T4WebFormsInjectionScopedContainer</ContainerType>
  <!--Types to generate constructors for.-->
  <IncludedTypes>
    <Type>System.Web.UI.MasterPage</Type>
    <Type>System.Web.UI.Page</Type>
    <Type>System.Web.UI.UserControl</Type>
    <Type>System.Web.IHttpHandler</Type>
    <Type>System.Web.IHttpModule</Type>
    <Type>System.Web.Services.WebService</Type>
  </IncludedTypes>
  <!--If true, the template output will be split into multiple files.-->
  <SplitIntoMultipleFiles>False</SplitIntoMultipleFiles>
  <!--If true, the template output will include documentation for the constructor.-->
  <IncludeDocumentation>True</IncludeDocumentation>
  <!--If true, the template output will include a registration list of all items to be injected.-->
  <OutputRegistrationList>False</OutputRegistrationList>
</Settings>